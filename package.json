{
  "name": "kros-log-viewer",
  "displayName": "KROS Log Viewer",
  "description": "Enhanced Visual Studio Code extension for viewing and analyzing KROS application logs with advanced filtering and visualization capabilities.",
  "version": "1.0.0",
  "publisher": "kros",
  "author": {
    "name": "Miroslav Burgyan",
    "email": "mirobrugyan@outlook.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/Burgyn/MMLib.LogsReader#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Burgyn/MMLib.LogsReader.git"
  },
  "bugs": {
    "url": "https://github.com/Burgyn/MMLib.LogsReader/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Debuggers"
  ],
  "keywords": [
    "log",
    "viewer",
    "kros",
    "debugging",
    "analysis",
    "logs",
    "log-viewer",
    "log-analysis",
    "filtering",
    "visualization",
    "typescript",
    "webview"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "krosLogViewer.showLog",
        "title": "Show KROS Log",
        "category": "KROS"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "krosLogViewer.showLog",
          "when": "resourceExtname == .log",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
} 